#include <iostream>
using namespace std;

int main() {
int arr[]={2,5,6,10,20};
   
    int n=sizeof(arr)/sizeof(arr[0]);
    int low=0;
    int val=10;
    int high=n-1;
    int index=-1;
    while (low<=high){
        int mid=(low+high)/2;
        if (arr[mid]==val){
            index=mid;
            break;
        }
        else if(arr[mid]<val){
          low=mid+1;  
        }
        
        else{
            high=mid-1;
            
        }
        
    }
    
    if(index==-1){
        
    cout<<"not fund"<<endl;}
    else{
        
   cout<<"fund at index "<<index;}
}
    
    



